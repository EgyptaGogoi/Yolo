{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/helmet-numberplate-detection/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/helmet-numberplate-detection/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/helmet-numberplate-detection",
  "projects": {
    "helmet-numberplate-detection": {
      "type": "application",
      "root": "apps/helmet-numberplate-detection",
      "entryFile": "main",
      "sourceRoot": "apps/helmet-numberplate-detection/src",
      "compilerOptions": {
        "tsConfigPath": "apps/helmet-numberplate-detection/tsconfig.app.json"
      }
    },
    "helmet-numberplate-frontend": {
      "type": "application",
      "root": "apps/helmet-numberplate-frontend",
      "entryFile": "main",
      "sourceRoot": "apps/helmet-numberplate-frontend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/helmet-numberplate-frontend/tsconfig.app.json"
      }
    }
  }
}